@using AppUnipsico.Utilities
@model Consulta

<div class="container-fluid">
    <div class="p-3 mb-3">
        <h3 class="text-left" style="color: white">Detalhes da consulta</h3>
    </div>
    <hr />
    <div class="card">
        <div class="card-body">
            <p class="card-text"><strong>Id da Consulta:</strong> @Model.ConsultaId</p>
            <p class="card-text"><strong>Data da Consulta:</strong> @Model.DataConsulta.Data</p>
            <p class="card-text"><strong>Nome do Paciente:</strong> @Model.Usuario.Email</p>
            <p class="card-text"><strong>CPF do Paciente:</strong> @FormatUtility.InserePontoTracoCpf(Model.Usuario.Cpf)</p>
            <p class="card-text"><strong>Data de Nascimento do Paciente:</strong> @Model.Usuario.DataNascimento</p>
            <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Voltar para a página inicial</a>
        </div>
    </div>
</div>
